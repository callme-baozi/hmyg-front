@charset "UTF-8";
/* 水平间距 */
/* 水平间距 */
.search-box {
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 1;
  background-color: #1296db;
}
.sugg-list {
  padding: 0 5px;
}
.sugg-list .sugg-item {
  font-size: 16px;
  padding: 13px 0;
  border-bottom: 1px solid #efefef;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.sugg-list .sugg-item .goods-name {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-right: 3px;
}
.history-box {
  padding: 0 5px;
}
.history-box .history-title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 40px;
  font-size: 16px;
  border-bottom: 1px solid #aaaa7f;
}
.history-box .history-list {
  padding: 5px 3px;
  display: flex;
  flex-wrap: wrap;
}
.history-box .history-list .uni-tag {
  margin-top: 5px;
  margin-right: 5px;
  font-size: 16px;
  color: #000000;
  background-color: #efefef;
  border-color: #efefef;
  display: flex;
}

